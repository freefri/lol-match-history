<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>lolstats@freefri.addons.mozilla.org</em:id>
    <em:version>0.1.0</em:version>
    <em:bootstrap>true</em:bootstrap>
    <em:type>2</em:type>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>32.0</em:minVersion>
        <em:maxVersion>35.*</em:maxVersion>
      </Description>
    </em:targetApplication>
   
    <!-- Front End MetaData -->
    <em:name>LOL match history stats</em:name>
    <em:description>Downloads your private match history of League of Legends and shows stats</em:description>
    <em:creator>Freefri</em:creator>
    <em:homepageURL>https://github.com/freefri/lol-match-history</em:homepageURL>
  </Description>      
</RDF>
